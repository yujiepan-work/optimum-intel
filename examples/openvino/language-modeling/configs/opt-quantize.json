  {
    "algorithm": "quantization",
    "export_to_onnx_standard_ops": false,
    "ignored_scopes": [
      "{re}.*Embedding*",
      "{re}.*__add___*",
      "{re}.*layer_norm_*",
      // "{re}.*bmm_1",
      // "{re}.*__truediv__*"
    ],
    "initializer": {
      "batchnorm_adaptation": {
        "num_bn_adaptation_samples": 0
      },
      "range": {
        "num_init_samples": 8,
        // "num_init_samples": 300,
        "type": "mean_min_max"
      }
    },
    "overflow_fix": "disable",
    // "preset": "mixed",
    "scope_overrides": {
      "activations": {
        // "{re}.*bmm_0": {
        //   "mode": "symmetric"
        // }
        "activations":
        {
            "mode": "symmetric"
        },
        "weights":
        {
            "mode": "symmetric",
            // "signed": true,
            // "per_channel": false
        }
      }
    }
  }
