{
    "algorithm": "quantization",
    "preset": "mixed",
    "overflow_fix": "disable",
    "initializer": {
        "range": {
            "num_init_samples": 128,
            "type": "percentile",
            "params": {
                "min_percentile": 0.01,
                "max_percentile": 99.99
            }
        },
        "batchnorm_adaptation": {
            "num_bn_adaptation_samples": 16
        }
    },
    "scope_overrides": {
        "activations": {
            "{re}.*matmul_0": {
                "mode": "symmetric"
            }
        }
    },
    "ignored_scopes": [
        "{re}.*__add___[0-1]",
        "{re}.*__truediv__*",
    ],
}